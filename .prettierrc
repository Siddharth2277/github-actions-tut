{
	"useTabs": true,
	"singleQuote": false,
	"trailingComma": "none",
	"printWidth": 100,
	"plugins": ["prettier-plugin-svelte"],
	"pluginSearchDirs": ["."],
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }],
	"svelteStrictMode": false,
	"bracketSpacing": true,
	"svelteSortOrder": "scripts-options-styles-markup",
	"quoteProps": "as-needed",
	"semi": false
}
